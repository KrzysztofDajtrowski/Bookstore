<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="Bookstore"
                   namespace="Bookstore.Models.Domain">
  <class name="OrdersBook" table="BookOrders" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="int">
      <generator class="native" />
    </id>

    <property name="OrderID" type="int" column="OrderID" />
    <property name="BookID" type="int" column="BookID" />

  </class>
</hibernate-mapping>